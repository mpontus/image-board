{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "variable-name": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "prettier": true
  }
}
